{
  "name": "ui",
  "version": "0.0.0",
  "license": "MIT",
  "description": "T-Drive 2",
  "keywords": [
    "angular",
    "components",
    "reusable"
  ],
  "scripts": {
    "start": "ng serve",
    "proxy-server": "ng serve --proxy-config proxy.conf.json",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "prod": "ng build --env=prod --output-path=../resources/static",
    "reinstall": "rm -rf node_modules tmp deploy dist && npm i",
    "start-api": "cd mock-api && covalent-data &",
    "stop-api": "pkill covalent-data",
    "webdriver-update": "bash ./node_modules/.bin/webdriver-manager update",
    "test": "ng test --code-coverage --single-run",
    "ghpages-deploy": "ng build --base-href /ui/ --aot -prod --sourcemap=false && bash scripts/ghpages-deploy"
  },
  "engines": {
    "node": ">4.4 < 7",
    "npm": ">3"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/cdk": "^2.0.0-beta.10",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "2.0.0-beta.10",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.2.0",
    "@angular/router": "^4.0.0",
    "@covalent/core": "1.0.0-beta.7",
    "@covalent/dynamic-forms": "1.0.0-beta.7",
    "@covalent/highlight": "1.0.0-beta.7",
    "@covalent/http": "1.0.0-beta.7",
    "@covalent/markdown": "1.0.0-beta.7",
    "@swimlane/ngx-charts": "^5.3.0",
    "angular2-jwt": "^0.2.3",
    "angular2-moment": "^1.6.0",
    "auth0-js": "^8.7.0",
    "core-js": "^2.4.1",
    "d3": "^4.9.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "9.10.0",
    "ngx-clipboard": "^8.1.1",
    "rxjs": "^5.4.1",
    "showdown": "1.6.4",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.2",
    "@angular/cdk": "^2.0.0-beta.10",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "@types/hammerjs": "2.0.30",
    "@types/selenium-webdriver": "2.53.36",
    "karma-firefox-launcher": "1.0.0",
    "phantomjs-prebuilt": "2.1.7"
  },
  "optionalDependencies": {
    "@covalent/data": "0.7.0"
  }
}
